<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taotao.manage.mapper.content.ContentMapper">
    <select id="queryContentByCategoryId" resultType="Content">
        SELECT * FROM TB_CONTENT WHERE CATEGORY_ID =  #{ID} ORDER BY UPDATED DESC
    </select>
</mapper>